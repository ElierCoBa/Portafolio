@model GestorDeSorteosEnLinea.Model.Sorteos

@{
    ViewBag.Title = "Detalles del sorteo";
}

<h2>Detalles del sorteo</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Fecha)
        </dd>
        <dt>
            @Html.DisplayName("Total vendido")
        </dt>

        <dd>
            @Html.Label((String)ViewBag.TotalVendido)
        </dd>
        <dt>
            @Html.DisplayName("Total pagado en premios")
        </dt>

        <dd>
            @Html.Label((String)ViewBag.TotalPagadoEnPremios)
        </dd>
        <dt>
            @Html.DisplayName("Total pagado al primer número en premios")
        </dt>

        <dd>
            @Html.Label((String)ViewBag.TotalPagadoPrimerNumero)
        </dd>
        <dt>
            @Html.DisplayName("Total pagado al segundo número en premios")
        </dt>

        <dd>
            @Html.Label((String)ViewBag.TotalPagadoSegundoNumero)
        </dd>
        <dt>
            @Html.DisplayName("Total pagado al tercer número en premios")
        </dt>

        <dd>
            @Html.Label((String)ViewBag.TotalPagadoTercerNumero)
        </dd>
    </dl>
</div>
<p>
  
    @Html.ActionLink("Regresar", "Index")
</p>
